  "repository": {
    "type": "git",
    "url": "https://github.com/kova-js/kova.git"
  },
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=true npm run build",
    "prepare": "npm run key:generate && npm run prisma:generate",
    "key:generate": "ts-node scripts/key-generate",
    "prod": "ssr build && pm2 start pm2.config.js",
    "stop": "pm2 stop pm2.config.js",
    "deploy": "ssr build && ssr deploy",
    "prisma:generate": "npx prisma generate",
    "prisma:db:push": "npx prisma db push",
    "dev": "ssr start",
    "seed": "ts-node prisma/seed.ts",
    "start:vite": "ssr start --vite",
    "build": "ts-node scripts/version && ssr build",
    "lint": "eslint . --ext .js,.tsx,.ts --cache",
    "lint:fix": "eslint . --ext .js,.tsx,.ts --cache --fix"
  },
  "engines": {
    "node": "^14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c commitlint.config.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,css,md}": "prettier --write"
  }
}
