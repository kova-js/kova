  "repository": {
    "type": "git",
    "url": "https://github.com/kova-js/kova.git"
  },
  "scripts": {
    "analyze": "BUNDLE_ANALYZE=true npm run build",
    "kova": "kova",
    "key:generate": "ts-node scripts/key-generate",
    "prod": "ssr build && pm2 start pm2.config.js",
    "start:prod": "ssr build && NODE_ENV=production node ./dist/main.js",
    "stop": "pm2 stop pm2.config.js",
    "deploy": "ssr build && ssr deploy",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "clean": "rimraf dist build .kova",
    "dev": "yarn clean && DEBUG='prisma:client' ssr start",
    "seed": "ts-node prisma/seed.ts",
    "start:vite": "ssr start --vite",
    "build:api": "nest build",
    "prebuild": "rimraf dist build && ts-node scripts/version",
    "build": "ssr build",
    "lint": "eslint . --ext .js,.tsx,.ts --cache",
    "lint:fix": "eslint . --ext .js,.tsx,.ts --cache --fix"
  },
  "engines": {
    "node": "^14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -c commitlint.config.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,css,md}": "prettier --write"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
